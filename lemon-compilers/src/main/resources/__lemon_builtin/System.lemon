namespace lemon

enum ServiceType{ Local,Remote }

message Manifest{
    string              appName
    string              version
    string              description
    list<string>        actors
    string[10]          others
    int32               hello
}

message ManifestV2 :Manifest{
    required map<string,string>     properties
    list<list<string>>              actors2
    map<string,map<string,int32>>   properties2
}

service Lemon{
    Manifest manifest()
}
